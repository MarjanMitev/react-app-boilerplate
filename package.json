{
  "name": "react-app-boilerplate",
  "version": "1.0.0",
  "description": "Boiletplate app with React and Redux",
  "main": "index.js",
  "scripts": {
    "test": "karma start --log-level debug",
    "test:watch": "npm test -- --watch",
    "start-dev": "npm test && npm run build-dev && webpack-dev-server --history-api-fallback --hot --inline --progress --colors --port 3000",
    "start-prod": "npm test && npm run build-prod && webpack-dev-server --history-api-fallback --hot --inline --progress --colors --port 3000",
    "build-dev": "webpack --dev --progress --colors",
    "build-prod": "webpack --prod --progress --color"
  },

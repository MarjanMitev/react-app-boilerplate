    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-native-modules": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.7.2",
    "bootstrap-sass": "^3.3.6",
    "clean-webpack-plugin": "^0.1.8",
    "css-loader": "^0.23.1",
    "expect": "^1.15.2",
    "file-loader": "^0.8.5",
